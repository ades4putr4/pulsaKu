<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ades.pulsaku">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"></activity>
        <activity
            android:name=".inputDataBarang"
            android:label="Input Data Barang"
            android:parentActivityName=".home" />
        <activity
            android:name=".listBarang"
            android:label="Daftar Harga"
            android:parentActivityName=".home" />
        <activity android:name=".listTransaksi" />
        <activity android:name=".Pembayaran" />
        <activity
            android:name=".home"
            android:label="Home"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Transaksi"></activity>
    </application>

</manifest>